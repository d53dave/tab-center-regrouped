    "url": "https://framagit.org/ariasuni/tabcenter-reborn"
  },
  "homepage": "https://framagit.org/ariasuni/tabcenter-reborn#readme",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "eslint": "^5.15.3",
    "web-ext": "^3.0.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  }
}
